<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <android.support.v7.widget.CardView
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_margin="5dp"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="8dp">

            <ImageView
                android:id="@+id/ivRouteType"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16dp"
                android:src="@drawable/tracetype_run"/>

            <!--<TextView-->
                <!--android:id="@+id/tv_name"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_centerVertical="true"-->
                <!--android:layout_toRightOf="@+id/iv_icon"-->
                <!--android:text="Name"/>-->

            <TextView
                android:id="@+id/tvRouteDistance"
                android:layout_width="wrap_content"
                android:layout_height="36dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="72dp"
                android:gravity="center"
                android:text="12.44"
                android:textColor="@color/colorMainViewCardBackground"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="130dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="km"
                android:textColor="@color/colorMainViewBackground"
                android:textSize="15sp"
                app:layout_constraintBottom_toBottomOf="@+id/tvRouteDistance"
                app:layout_constraintStart_toEndOf="@+id/tvRouteDistance"
                app:layout_constraintTop_toTopOf="@+id/tvRouteDistance" />


            <TextView
                android:id="@+id/tvRouteTimeLast"
                android:layout_width="115dp"
                android:layout_height="30dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="16dp"
                android:drawableLeft="@drawable/trace_time"
                android:gravity="center"
                android:text="00:47:00"
                android:textColor="@color/colorMainViewBackground"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/textView2" />



        </RelativeLayout>
    </android.support.v7.widget.CardView>
</FrameLayout>
